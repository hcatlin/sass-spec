<===> options.yml
---
:ignore_for:
- libsass

<===>
<==============================================================================>
<===> extending_an_extender_multiple_times/extendee.scss
.extendee {a: 1}

<===> extending_an_extender_multiple_times/extender.scss
@use "extendee";
.extender {@extend .extendee}

<===> extending_an_extender_multiple_times/extender_of_extender1.scss
@use "extender";
.extender_of_extender1 {
  @extend .extender;
  b: 1;
}

<===> extending_an_extender_multiple_times/extender_of_extender2.scss
@use "extender";
.extender_of_extender2 {
  @extend .extender;
  b: 2;
}

<===> extending_an_extender_multiple_times/input.scss
@use "extender_of_extender1";
@use "extender_of_extender2";

<===> extending_an_extender_multiple_times/output.css
.extendee, .extender, .extender_of_extender2, .extender_of_extender1 {
  a: 1;
}

.extender_of_extender1 {
  b: 1;
}

.extender_of_extender2 {
  b: 2;
}

<===>
<==============================================================================>
<===> extending_an_extend_within_pseudoselector/matches/options.yml
---
:todo:
- sass/dart-sass#1297

<===> extending_an_extend_within_pseudoselector/matches/extendee.scss
.extendee {a: 1}

<===> extending_an_extend_within_pseudoselector/matches/extender.scss
@use "extendee";
:matches(.extender) {@extend .extendee}

<===> extending_an_extend_within_pseudoselector/matches/input.scss
@use "extender";
.extender_of_extender {
  @extend .extender;
  b: 1;
}

<===> extending_an_extend_within_pseudoselector/matches/output.css
.extendee, :matches(.extender, .extender_of_extender) {
  a: 1;
}

.extender_of_extender {
  b: 1;
}
